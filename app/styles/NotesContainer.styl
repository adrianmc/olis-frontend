@import "application"

#notes-container
  width 100%
  height 100%
  padding 6px 18px
  position relative
  
  .notes-data-wrapper *
    font-family Cardo, Georgia, Serif
    overflow scroll
  
  .notes-icon-bar
    position absolute
    top 18px
    right 18px
    display flex
    display -webkit-flex
    
    .notes-icon 
      cursor pointer
      margin 4px